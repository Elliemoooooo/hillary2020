function updateElements() {
    // Remove default candidate summary
    const candidate_summary = document.getElementById("candidate_summary");
    if (candidate_summary && candidate_summary.childElementCount > 1) {
        candidate_summary.removeChild(candidate_summary.firstElementChild);
    }

    // Remove default running mate summary
    const running_mate_summary = document.getElementById("running_mate_summary");
    if (running_mate_summary && running_mate_summary.childElementCount > 1) {
        running_mate_summary.removeChild(running_mate_summary.firstElementChild);
    }
}

var mutationObserver = new MutationObserver(updateElements);
mutationObserver.observe(document.documentElement, { childList: true, subtree: true });

