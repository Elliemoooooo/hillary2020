  playPauseButton.addEventListener("click", function () {
    const audio = document.getElementById("audio");
    if (audio.paused) {
      audio.play();
      playPauseButton.src = "https://files.catbox.moe/358r8p.png"; // Set the pause image
    } else {
      audio.pause();
      playPauseButton.src = "https://files.catbox.moe/8m2l18.png"; // Set the play image
    }
  });
  
